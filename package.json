{
  "name": "belsky-in",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "watch": "tsc -w",
    "serve": "nodemon dist/server.js",
    "start": "concurrently \"npm:watch\" \"npm:serve\"",
    "build": "tsc && cp index.html dist/",
    "serve:prod": "node dist/server.js",
    "start:prod": "npm run build && npm run serve:prod",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.5",
    "@types/winston": "^2.4.4",
    "express": "^4.17.1",
    "typescript": "^3.9.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "tslint": "^6.1.2"
  }
}
